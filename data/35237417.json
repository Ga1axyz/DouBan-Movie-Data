u8c8c\u3002\u8ddf\u968f\u7740\u79d1\u5b66\u5bb6\u7684\u8db3\u8ff9\uff0c\u6211\u4eec\u5c06\u770b\u5230\u5728\u4e07\u4f59\u5e74\u7684\u65f6\u7a7a\u5c3a\u5ea6\u4e2d\uff0c\u4eba\u7c7b\u5728\u8fd9\u91cc\u53cd\u590d\u51fa\u73b0\uff0c\u53c8\u90fd\u795e\u79d8\u6d88\u5931\u3002\u7f57\u5e03\u6cca\u7684\u6e38\u79fb\u76c8\u7f29\u4e0e\u591a\u79cd\u6781\u7aef\u5730\u8c8c\uff0c\u6b64\u6d88\u5f7c\u957f\u3002\u5c0f\u6cb3\u4eba\u7fa4\u66fe\u7ecf\u7684\u5bb6\u56ed\uff0c\u697c\u5170\u53e4\u57ce\u7684\u5174\u8870\u5b58\u4ea1\u3002\u6ca1\u6709\u4eba\u6000\u7591\u8fc7\u201c\u4e1d\u7ef8\u4e4b\u8def\u201d\u7684\u5b58\u5728\uff0c\u4f46\u4e5f\u6ca1\u6709\u4eba\u76f8\u4fe1\uff0c\u4e24\u5343\u591a\u5e74\u4e4b\u540e\u7684\u4eca\u5929\uff0c\u6211\u4eec\u8fd8\u80fd\u811a\u8e0f\u5728\u8fd9\u6761\u4f20\u5947\u7684\u53e4\u9053\u4e0a\u3002\u8fd9\u4e00\u5207\uff0c\u90fd\u53d8\u5316\u5267\u70c8\u800c\u53c8\u5145\u6ee1\u4e86\u672a\u77e5\u7684\u620f\u5267\u6027\u3002\n\u7efc\u5408\u79d1\u8003\u7684\u961f\u5458\u4eec\uff0c\u4ee5\u79d1\u5b66\u7684\u8ba4\u77e5\uff0c\u8bb2\u8ff0\u81ea\u5df1\u773c\u4e2d\u7f57\u5e03\u8352\u539f\u4e0e\u4eba\u7c7b\u6d3b\u52a8\u7684\u53d8\u8fc1\u3002\u4ed6\u4eec\u6240\u7ecf\u5386\u4e0e\u63cf\u7ed8\u51fa\u7684\u7f57\u5e03\u8352\u539f\uff0c\u4e5f\u662f\u4e00\u5904\u4eba\u7c7b\u5ba1\u89c6\u81ea\u8eab\u547d\u8fd0\u6f14\u53d8\u7684\u7edd\u4f73\u6837\u677f\u3002\n\u5206\u96c6\u7b80\u4ecb\uff1a\n\u7b2c\u4e00\u96c6  \u697c\u5170\u7684\u6a21\u6837\n1900\u5e741\u6708\uff0c\u745e\u5178\u63a2\u9669\u5bb6\u65af\u6587\u00b7\u8d6b\u5b9a\u5728\u63a2\u9669\u8003\u5bdf\u65f6\u610f\u5916\u5730\u53d1\u73b0\u4e86\u7f57\u5e03\u8352\u539f\u4e2d\u7684\u697c\u5170\u53e4\u57ce\u3002\u8fd9\u4e4b\u540e\uff0c\u4e24\u5343\u5e74\u524d\u201c\u5931\u843d\u7684\u6587\u660e\u201d\u5438\u5f15\u7740\u5404\u56fd\u63a2\u9669\u5bb6\u7eb7\u81f3\u6c93\u6765\uff0c\u4f46\u76f4\u5230\u4e00\u767e\u591a\u5e74\u4e4b\u540e\u7684\u4eca\u5929\uff0c\u5173\u4e8e\u8fd9\u5ea7\u53e4\u57ce\u7684\u771f\u76f8\u4ecd\u7136\u4f17\u8bf4\u7eb7\u7ead\u3002\n\u8fd9\u4e00\u6b21\u7efc\u5408\u79d1\u8003\uff0c\u6211\u4eec\u8ddf\u968f\u79d1\u5b66\u5bb6\u4eec\uff0c\u8d70\u8fdb\u697c\u5170\u53e4\u57ce\uff0c\u5c1d\u8bd5\u56de\u7b54\u90a3\u4e9b\u957f\u4e45\u7684\u7591\u95ee\u3002\u8fd9\u4e2a\u5174\u76db\u4e00\u65f6\u7684\u4eba\u7c7b\u5bb6\u56ed\uff0c\u4e3a\u4f55\u4f1a\u7a81\u7136\u6d88\u4ea1\uff1f\u8fd9\u5ea7\u53e4\u57ce\u9057\u8ff9\u7a76\u7adf\u662f\u4e0d\u662f\u697c\u5170\uff0c\u90a3\u662f\u4e00\u5ea7\u4ec0\u4e48\u65f6\u5019\u7684\u57ce\uff0c\u4f1a\u662f\u4ec0\u4e48\u4eba\u4f4f\u5728\u91cc\u9762\uff1f\n\u5728\u73b0\u4ee3\u79d1\u5b66\u7684\u53d6\u8bc1\u7814\u7a76\u4e4b\u4e0b\uff0c\u8fd9\u5ea7\u53e4\u57ce\u5f53\u5e74\u7684\u6a21\u6837\u9010\u6e10\u6e05\u6670\u8d77\u6765\u2026\u2026\n\u7b2c\u4e8c\u96c6  \u65e0\u4eba\u533a\n\u5e7f\u9614\u7684\u7f57\u5e03\u6cca\u65e0\u4eba\u533a\u4e2d\uff0c\u6563\u843d\u7740\u5404\u79cd\u65f6\u4ee3\u7684\u4eba\u7c7b\u6d3b\u52a8\u75d5\u8ff9\u3002\u4e3a\u4e86\u5f04\u6e05\u4ed6\u4eec\u7684\u5173\u7cfb\uff0c\u79d1\u8003\u961f\u5458\u5728\u8352\u539f\u4e2d\uff0c\u5c55\u5f00\u4e86\u53f2\u65e0\u524d\u4f8b\u7684\u7f51\u683c\u5316\u5927\u641c\u7d22\u3002\n\u672c\u4e16\u7eaa\u521d\uff0c\u201c\u5c0f\u6cb3\u5893\u5730\u201d\u4e2d\u201c\u5c0f\u6cb3\u516c\u4e3b\u201d\u7684\u6316\u6398\uff0c\u66fe\u5f15\u53d1\u4e86\u5e7f\u6cdb\u7684\u5173\u6ce8\u3002\u8fd9\u6b21\u7efc\u5408\u79d1\u8003\uff0c\u7ec8\u4e8e\u627e\u5230\u4e86\u201c\u5c0f\u6cb3\u65f6\u671f\u201d\u4eba\u7fa4\u7684\u5c45\u5740\u3002\n\u9664\u4e86\u719f\u6089\u7684\u697c\u5170\u4eba\u548c\u5c0f\u6cb3\u4eba\uff0c\u79d1\u8003\u961f\u5458\u4e00\u6b21\u53c8\u4e00\u6b21\u4e0e\u5386\u53f2\u4e0a\u964c\u751f\u7684\u201c\u4eba\u4eec\u201d\u76f8\u9047\u3002\n\u5728\u8fd9\u91cc\uff0c\u6211\u4eec\u770b\u5230\u4e863\u4e07\u5e74\u7684\u65f6\u95f4\u91cc\uff0c\u4e0d\u540c\u7684\u4eba\u7fa4\u5728\u7f57\u5e03\u8352\u539f\u6765\u6765\u5f80\u5f80\uff0c\u800c\u4eba\u7c7b\u6587\u660e\u7a81\u7136\u4e2d\u65ad\u6d88\u4ea1\u7684\u697c\u5170\u6545\u4e8b\uff0c\u4e5f\u5728\u8fd9\u91cc\u5faa\u73af\u4e0a\u6f14\u3002\n\u7b2c\u4e09\u96c6 \u5730\u7403\u4e4b\u8033\n\u5f53\u7f57\u5e03\u6cca\u88ab\u4ece\u592a\u7a7a\u6295\u5c04\u7684\u76ee\u5149\u5173\u6ce8\uff0c\u5b83\u7684\u795e\u79d8\u5c31\u662f\u4e00\u5e45\u56fe\u6848\u2014\u2014\u201c\u5730\u7403\u4e4b\u8033\u201d\u3002\u8fd9\u4e2a\u201c\u8033\u6735\u201d\u662f\u5982\u4f55\u5f62\u6210\uff1f\u8fd9\u4e2a\u7591\u95ee\uff0c\u4e00\u76f4\u56f0\u6270\u7740\u536b\u661f\u9065\u611f\u9886\u57df\u7684\u4e13\u5bb6\u2014\u2014\u90b5\u82b8\u3002\u5e26\u7740\u5bf9\u771f\u76f8\u7684\u6e34\u671b\uff0c\u4e94\u5e74\u95f4\uff0c\u5979\u548c\u5979\u7684\u5b66\u751f\u4eec\uff0c\u5728\u832b\u832b\u6c99\u6f20\u4e0a\u6316\u5751\u5228\u571f\uff0c\u91c7\u96c6\u5404\u79cd\u5730\u8868\u6570\u636e\uff0c\u540c\u65f6\u5229\u7528\u536b\u661f\u9065\u611f\u6280\u672f\uff0c\u53d1\u73b0\u4e86\u5730\u8868\u4e0b\u6c34\u7684\u75d5\u8ff9\uff0c\u4e3a\u4e86\u66f4\u7cbe\u786e\u5730\u9a8c\u8bc1\u536b\u661f\u9065\u611f\u6570\u636e\uff0c\u90b5\u82b8\u66f4\u662f\u5728\u6d59\u6c5f\u5fb7\u6e05\u642d\u5efa\u4e86\u4e00\u4e2a\u201c\u8fd1\u5730\u5b87\u5b99\u201d\u2014\u2014\u5fae\u6ce2\u9065\u611f\u6d4b\u91cf\u5b9e\u9a8c\u5ba4\u3002\u501f\u52a9\u79d1\u5b66\uff0c\u4ed6\u4eec\u6162\u6162\u6380\u5f00\u66fe\u7ecf\u7684\u5927\u6e56---\u7f57\u5e03\u6cca\u7684\u795e\u79d8\u9762\u7eb1\u3002\n\u7b2c\u56db\u96c6 \u957f\u8def\u6f2b\u6f2b\n\u8fd9\u6b21\u7efc\u5408\u79d1\u8003\u5237\u65b0\u4e86\u5f88\u591a\u8fc7\u53bb\u4eba\u4eec\u5bf9\u7f57\u5e03\u8352\u539f\u7684\u8ba4\u8bc6\u3002\u73af\u5883\u8003\u53e4\u5b66\u5bb6\u79e6\u5c0f\u5149\uff0c\u4f5c\u4e3a\u79d1\u8003\u961f\u7684\u961f\u957f\uff0c\u4ed6\u548c\u961f\u5458\u4e00\u8d77\uff0c\u83b7\u5f97\u4e86\u697c\u5170\u5730\u533a\u8bb8\u591a\u7a81\u7834\u6027\u7684\u65b0\u53d1\u73b0\u3002\u8fd9\u4e00\u6b21\uff0c\u79e6\u5c0f\u5149\u901a\u8fc7\u536b\u661f\u9065\u611f\u5730\u56fe\u53d1\u73b0\u4e86\u4e00\u6761\u6a2a\u8de8\u6566\u714c\u5230\u697c\u5170\u7684\u9053\u8def\u7684\u75d5\u8ff9\uff0c\u8fd9\uff0c\u83